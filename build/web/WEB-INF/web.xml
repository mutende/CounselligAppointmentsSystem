<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <!--login servlet-->
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ViewAllCounsellors</servlet-name>
        <servlet-class>dean.ViewAllCounsellors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deanhome</servlet-name>
        <servlet-class>supportservlet.deanhome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studenthome</servlet-name>
        <servlet-class>supportservlet.studenthome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDate</servlet-name>
        <servlet-class>student.AddDate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CounsellorSetSechedule</servlet-name>
        <servlet-class>counsellor.CounsellorSetSechedule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>counsellor.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchView</servlet-name>
        <servlet-class>counsellor.searchView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentprofile</servlet-name>
        <servlet-class>profile.studentprofile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DateManipulationServ</servlet-name>
        <servlet-class>counsellor.DateManipulationServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>booking</servlet-name>
        <servlet-class>student.booking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApproveSchedules</servlet-name>
        <servlet-class>dean.ApproveSchedules</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>succesBooking</servlet-name>
        <servlet-class>supportservlet.succesBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dean</servlet-name>
        <servlet-class>dean.dean</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterAppointmentsDao</servlet-name>
        <servlet-class>dean.FilterAppointmentsDao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterAppointments</servlet-name>
        <servlet-class>dean.FilterAppointments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterSchedule</servlet-name>
        <servlet-class>dean.FilterSchedule</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <!--end login servlet-->
    <!--signup servlet-->
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup.signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <!--end signupservlet-->
    <!--dean chagepass-->
    <servlet>
        <servlet-name>DeanChangePassword</servlet-name>
        <servlet-class>dean.DeanChanegePasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeanChangePassword</servlet-name>
        <url-pattern>/DeanChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CounsellorSetSechedule</servlet-name>
        <url-pattern>/CounsellorSetSechedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchView</servlet-name>
        <url-pattern>/searchView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDate</servlet-name>
        <url-pattern>/AddDate</url-pattern>
    </servlet-mapping>
    <!--end dean change servlet-->
    <!--login error servlet-->
    <servlet>
        <servlet-name>loginError</servlet-name>
        <servlet-class>supportservlet.loginError</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginError</servlet-name>
        <url-pattern>/loginError</url-pattern>
    </servlet-mapping>
    <!--end logi n error servlet-->
    <!--counsellor servlet-->
    <servlet>
        <servlet-name>CounsellorSignup</servlet-name>
        <servlet-class>counsellor.CounsellorSignup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CounsellorSignup</servlet-name>
        <url-pattern>/CounsellorSignup</url-pattern>
    </servlet-mapping>
    <!--end counsellor servlet-->
    <!--change password servlet-->
    <servlet>
        <servlet-name>changepassword</servlet-name>
        <servlet-class>profile.changepassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changepassword</servlet-name>
        <url-pattern>/changepassword</url-pattern>
    </servlet-mapping>
    <!--end of change pass servlet-->
    <!--loggout servlet-->
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>login.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <!--end logout servlet-->
    <!--indexpage servlet-->
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>supportservlet.home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewAllCounsellors</servlet-name>
        <url-pattern>/ViewAllCounsellors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deanhome</servlet-name>
        <url-pattern>/deanhome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studenthome</servlet-name>
        <url-pattern>/studenthome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentprofile</servlet-name>
        <url-pattern>/studentprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DateManipulationServ</servlet-name>
        <url-pattern>/DateManipulationServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApproveSchedules</servlet-name>
        <url-pattern>/ApproveSchedules</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>succesBooking</servlet-name>
        <url-pattern>/succesBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dean</servlet-name>
        <url-pattern>/dean</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterAppointmentsDao</servlet-name>
        <url-pattern>/FilterAppointmentsDao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterAppointments</servlet-name>
        <url-pattern>/FilterAppointments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterSchedule</servlet-name>
        <url-pattern>/FilterSchedule</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
